{
    "env": {
      "browser": true,
      "es6": true,
    },
    "extends": ["plugin:prettier/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error",
      "arrow-body-style": "off",
      "prefer-arrow-callback": "off"
    },
    "plugins": ["prettier"],
}